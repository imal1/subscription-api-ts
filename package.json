{
  "name": "subscription-api-ts",
  "version": "1.0.0",
  "description": "TypeScript订阅转换API服务 - 将sing-box配置转换为Clash订阅格式",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:with-alias": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev:watch": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "lint": "echo 'Linting not configured'",
    "test": "echo 'Tests not configured'",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "postbuild": "echo 'Build completed successfully'",
    "install:deps": "npm install",
    "setup": "npm run install:deps && npm run build",
    "nginx:config": "bash scripts/generate-nginx-config.sh",
    "nginx:setup": "bash scripts/setup-nginx.sh",
    "systemd:service": "bash scripts/generate-systemd-service.sh",
    "config:all": "bash scripts/generate-all-configs.sh",
    "config:validate": "node -e \"require('dotenv').config(); console.log('✅ 环境变量验证通过:', JSON.stringify(process.env, null, 2))\"",
    "config:check": "bash scripts/validate-paths.sh",
    "ts:diagnose": "bash scripts/diagnose-typescript.sh",
    "ts:fix": "bash scripts/fix-typescript.sh",
    "service:status": "bash scripts/check-service-status.sh",
    "service:fix": "bash scripts/fix-systemd.sh"
  },
  "keywords": [
    "subscription",
    "clash",
    "sing-box",
    "typescript",
    "api",
    "proxy",
    "vpn"
  ],
  "author": "imal1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/imal1/subscription-api-ts.git"
  },
  "bugs": {
    "url": "https://github.com/imal1/subscription-api-ts/issues"
  },
  "homepage": "https://github.com/imal1/subscription-api-ts#readme",
  "dependencies": {
    "axios": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "node-cron": "^3.0.2",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.4.5",
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
